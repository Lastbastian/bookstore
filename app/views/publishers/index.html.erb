<h2>Publishers</h2>

<table class="table table-striped">
  <tr>
    <th>Oublisher Name</th>
    <th>Actions</th>
  </tr>

  <% @publishers.each do |publisher| %>
    <tr>
      <td><%= publisher.name  %></td>
      <td><%= link_to 'Edit', edit_publisher_path(publisher.id) %> | <%= link_to 'Delete', publisher_path(publisher.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>